ext-align: center;\r\n}\r\n.icon {\r\n  height: 2rem;\r\n  width: 2rem;\r\n  font-size: 2rem;\r\n  line-height: 2rem;\r\n  top: 10vh;\r\n}\r\n.gear {\r\n  right: 5vh;\r\n}\r\n.plus {\r\n  left: 5vh;\r\n}\r\n\r\n.city-card {\r\n  top: 30%;\r\n  left: 0;\r\n  right: 0;\r\n  margin-left: auto;\r\n  margin-right: auto;\r\n  width: 40%;\r\n}\r\n\r\n.city-name {\r\n  font-weight: bold;\r\n}\r\n\r\n.temp {\r\n  font-size: 5rem;\r\n  line-height: 4.5rem;\r\n}\r\n.temp span {\r\n  font-weight: 300;\r\n  font-size: 4.5rem;\r\n}\r\n\r\n.w-type, .min-max {\r\n  font-weight: 300;\r\n  font-size: 1rem;\r\n}\r\n\r\n.min-max {\r\n  font-size: 0.7rem;\r\n}\r\n\r\n.text {\r\n  position: absolute;\r\n  top: 0;\r\n  right: calc(100vh-25vh / 2);\r\n  /* z-index: 12; */\r\n  width: 25vh;\r\n  display: flex;\r\n  justify-content: center;\r\n}\r\n\r\n.flex-container {\r\n  width: 100%;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: flex-end;\r\n}\r\n\r\n/* background section\r\n  ----------------------*/\r\n\r\n.background-slider {\r\n  height: 100vh;\r\n  width: 100vw;\r\n  background-color: green;\r\n}\r\n\r\n.background-img.one,\r\n.background-img.two {\r\n  height: 100vh;\r\n  width: 100vw;\r\n  min-height: 100%;\r\n  min-width: 100%;\r\n  background: var(--bg_main);\r\n  background-position: center center;\r\n  background-repeat: no-repeat;\r\n  background-size: cover;\r\n  background-color: #464646;\r\n  filter: blur(5px);\r\n  /* position: absolute; */\r\n  /* z-index: -1; */\r\n}\r\n\r\n.background-img.one {\r\n  background-image: url(asset/img/LosAngeles.jpg);\r\n}\r\n\r\n.background-img.two {\r\n  background-image: url(asset/img/sanFrancisco.jpg);\r\n}\r\n\r\n.background-img .city-list {\r\n  height: 100%;\r\n  width: 100%;\r\n}\r\n\r\n.background-img .city-el {\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: space-between;\r\n  height: 100vh;\r\n}\r\n\r\n.background-img .city-el .city-card {\r\n  display: flex;\r\n  flex-direction: 